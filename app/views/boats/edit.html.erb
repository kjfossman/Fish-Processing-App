<%= form_with(model: @boat) do |f| %>
    <%= render 'errors' %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>
    <%= f.label :captain %>
    <%= f.text_field :captain %><br>
    <%= f.label :home_port %>
    <%= f.text_field :home_port %><br>

    <%= f.label :fish_processor %>
    <%= f.collection_select :fish_processor_id, FishProcessor.all, :id, :name, prompt: true %><br>
   
    <%= f.submit "Update Boat" %>
<% end %>